package templates

templ NewClients(taking bool) {
	if taking {
		<p>
			For inquires, contact <span class="bilte-co">hello</span> [at] <span class="bilte-co">bilte.co</span>.
		</p>
	} else {
		<p>
			At this time, <span class="bilte-co">bilte co</span> is not
			accepting new clients or projects.
		</p>
	}
}
